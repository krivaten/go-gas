{
  "name": "go-gas-client",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel start:app start:server",
    "start:app": "PORT=8080 node server.js",
    "start:server": "netlify-lambda serve src/lambda",
    "build": "npm-run-all --parallel build:**",
    "build:app": "parcel build src/index.html",
    "build:server": "netlify-lambda build src/lambda",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --runInBand --coverage --silent"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krivaten/go-gas.git"
  },
  "author": "Kris Van Houten <krivaten@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/krivaten/go-gas/issues"
  },
  "homepage": "https://github.com/krivaten/go-gas#readme",
  "devDependencies": {
    "@types/jest": "^23.3.0",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "babel-core": "^6.26.3",
    "babel-preset-airbnb": "^2.5.2",
    "cssnano": "^4.0.1",
    "express": "^4.16.3",
    "http-proxy-middleware": "^0.18.0",
    "jest": "^23.4.1",
    "jest-junit": "^5.1.0",
    "netlify-lambda": "^0.4.0",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "parcel": "^1.9.7",
    "parcel-bundler": "^1.9.7",
    "parcel-plugin-typescript": "^1.0.0",
    "prettier": "^1.13.7",
    "react-testing-library": "^4.1.2",
    "ts-jest": "^23.0.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "apollo-boost": "^0.1.12",
    "apollo-server-lambda": "^2.0.0",
    "dotenv": "^6.0.0",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.5",
    "mongodb": "^3.1.1",
    "react": "^16.4.1",
    "react-apollo": "^2.1.9",
    "react-dom": "^16.4.1"
  },
  "jest": {
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/../../node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(test|spec).(ts|tsx|js)"
    ]
  }
}
